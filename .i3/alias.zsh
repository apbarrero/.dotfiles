alias 4k="xrandr --output HDMI-2 --mode 3840x2160 --above eDP-1"
alias dell="xrandr --output HDMI-2 --mode 2560x1440 --above eDP-1"
alias hp="xrandr --output HDMI-2 --mode 1920x1080 --above eDP-1"
alias mirror="xrandr --output HDMI-2 --mode 1920x1080 --same-as eDP-1"
alias thinkpad="xrandr --output HDMI-2 --off"

